let isDivisible=fn(n,by){
    let times=n/by
    times*by==n
};

let isPrimeHelper = fn(i,n) {
  if (i < 2) { return true }
  if( isDivisible(n,i)){return false}
  isPrimeHelper(i - 1,n)
};

let isPrime=fn(n){
    return if(n<2) {
      false
    }else{
      isPrimeHelper(n-1,n)
    }
};

isPrime(-1);
